<?xml version="1.0" encoding="UTF-8"?>
<CtxImport>

	<Global>
		<LoginIP>localhost</LoginIP>
		<LoginPort>29000</LoginPort>
		<LoginUser>admin</LoginUser>
		<LoginPW>admin</LoginPW>
		<ImportModus>u</ImportModus>
	</Global>
  
	<Linker>
	
		<!-- link inventory to Set -->
		<Link>@InvSet=SetNum</Link>

		<!-- link set to theme -->
		<Link>@SetThm=IDtheme</Link>

		<!-- link inventory-parts to inventory, part and colour -->
		<Link>@InPInv=IDInv</Link>
		<Link>@InPPrt=PrtNum</Link>
		<Link>@InPCol=IDCol</Link>

		<!-- link inventory-set to inventory an set -->
		<Link>@IStInv=IDInv</Link>
		<Link>@IStSet=SetNum</Link>

		<!-- link part to part-category -->
		<Link>@PrtCat=IDcat</Link>

		<!-- link theme to theme-parent -->
		<Link>@ThmThm=IDtheme</Link>
		
	</Linker>
  
</CtxImport>
